{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "target": "ES5",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": true,
    "lib": [
      "ES2019.Array",
      "DOM"
    ],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "@lib/*": [
        "src/lib/*"
      ],
      "@typings/*": [
        "src/typings/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@breng/*": [
        "src/breng/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@components": [
        "src/components"
      ],
      "@hooks": [
        "src/utils/hooks"
      ],
      "@server": [
        "src/utils/server"
      ],
      "@seo-default": [
        "src/next-seo.config"
      ]
    },
    "allowJs": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
